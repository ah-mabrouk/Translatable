{
    "name": "mabrouk/translatable",
    "description": "a laravel package to handle dealing with dynamic translations",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Mabrouk",
            "email": "a.mabrouk25@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": ">=5.8"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mabrouk\\Translatable\\TranslatableServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Mabrouk\\Translatable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
